{
    "manifest_version":3,
    "name": "dark ex2",
    "version":"2.0.0",
"update_url": "https://clients2.google.com/service/update2/crx",
"author":" Dark Mode",

"background":{
    "service_worker":"js/background.js"},

"action":{"default_popup":"popup/popup.html"
} ,


"default_locale":"en",
"description":"Turn you browser to the dark. ex2 is a neat dark theme for websites",
"icons": {
    "16": "icons/icon-z.png",
    "48": "icons/icon-z.png",
    "128": "icons/icon-z.png"
},

"minimum_chrome_version":"102.0",

"permissions":["storage","contextMenus","scripting","alarms"],
"host_permissions":["<all_urls>"],

"web_accessible_resources":[{"resources":["js/proxy.js"],
"matches":["<all_urls>"


]}


]}