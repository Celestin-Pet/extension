
{
  "name": "Dark Mode Extension",
  "description": "Adds a dark mode to web pages",  
  "version": "1.0",
  "manifest_version": 2,
    "background": {
    "scripts": ["background.js"], 
     "parsistent": true
      
    },
 
   
    

    
  "icons": {
        "48": "1-icon.png"
  },
  "browser_action": {
    "default_name": "Dark Mode Extension",
    "default_popup": "popop2.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
   

  ]
}
